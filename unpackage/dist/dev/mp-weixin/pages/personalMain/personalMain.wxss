page {
  background: #f8f8f8;
  letter-spacing: 1px;
}
.content {
  background-color: #f6b4a6;
}
.avatar {
  margin: 80rpx 60rpx 30rpx 90rpx;
}
.empty {
  margin: 10rpx;
  height: 10rpx;
}
.nickName {
  margin-top: 60rpx;
  font-family: "Times New Roman", Times, serif;
  font-weight: 600;
  color: rgba(32, 41, 41, 0.76);
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
}
.list-icon {
  position: relative;
  left: 30%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.mood-list {
  display: flex;
  justify-content: space-between;
  border-radius: 10rpx;
  width: 90%;
  margin: 20rpx 20rpx;
  padding: 8rpx 12rpx;
  background-color: #F8E8E1;
}
.person {
  display: flex;
}
.mood-dairy {
  position: relative;
  height: 430rpx;
  background: #fff;
  margin: 15rpx 40rpx;
  padding: 15rpx;
  border-radius: 10rpx;
  box-shadow: 0px 2px 8px #6d6d6d;
  /*下边阴影*/
}
.flexMood {
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.toDoListHead {
  display: flex;
  justify-content: start;
  margin-left: 20rpx;
}
.mood-font {
  display: flex;
  border-radius: 10rpx;
  width: 300rpx;
  background-color: #f0f0f0;
}
.mood-word {
  color: #dc8c6b;
  font-size: 35rpx;
  font-weight: 300;
}
.mood-word-finish {
  color: #dc8c6b;
  font-size: 35rpx;
  font-weight: 300;
  text-decoration: line-through;
}
.dateTime {
  color: #dc8c6b;
  font-size: 30rpx;
  font-weight: 300;
}
.dairyDetail {
  margin: 5rpx 0;
  font-size: 30rpx;
}
.dairyDetailEdit {
  position: absolute;
  bottom: 10%;
  right: 5%;
}
.to-do-list {
  position: relative;
  font-size: 30rpx;
  height: 400rpx;
  background: #fff;
  margin: 25rpx 40rpx;
  padding: 15rpx;
  border-radius: 10rpx;
  box-shadow: 0px 2px 8px #6d6d6d;
}

