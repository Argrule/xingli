page {
  background: #f8f8f8;
  letter-spacing: 1px;
}
.content {
  background-color: #f6b4a6;
}
.avatar {
  margin: 10rpx 60rpx 50rpx 90rpx;
}
.empty {
  margin: 10rpx;
  height: 10rpx;
}
.nickName {
  margin-top: 15rpx;
  font-family: "Times New Roman", Times, serif;
  font-weight: 600;
  color: rgba(32, 41, 41, 0.76);
}
.list-dot-icon {
  position: absolute;
  right: 10%;
  top: -2%;
}
.arrow-right-icon {
  margin: 50rpx;
  position: absolute;
  top: 70rpx;
  right: 4%;
}
.person {
  display: flex;
}
.mood-dairy {
  height: 330rpx;
  background: #fff;
  margin: 15rpx 40rpx;
  padding: 15rpx;
  border-radius: 10rpx;
  box-shadow: 0px 2px 8px #6d6d6d;
  /*下边阴影*/
}
.mood-font {
  display: flex;
  border-radius: 10rpx;
  width: 350rpx;
  background-color: #f0f0f0;
}
.mood-word {
  color: #dc8c6b;
  font-size: 35rpx;
  font-weight: 300;
}
.dateTime {
  color: #dc8c6b;
  font-size: 30rpx;
  font-weight: 300;
  position: absolute;
  right: 6%;
  top: 24%;
}
.dairyDetail {
  margin: 5rpx 0;
  font-size: 30rpx;
}
.to-do-list {
  font-size: 30rpx;
  height: 400rpx;
  background: #fff;
  margin: 25rpx 40rpx;
  padding: 15rpx;
  border-radius: 10rpx;
  box-shadow: 0px 2px 8px #6d6d6d;
}

